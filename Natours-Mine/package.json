{
  "name": "natours-mine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "devserver": "live-server",
    "watch:sass": "sass --watch --no-source-map ./sass/main.scss ./styles/main.css",
    "compile:sass": "sass --style=compressed --no-source-map ./sass/main.scss ./styles/main.min.css",
    "prefix": "postcss ./styles/main.min.css --use autoprefixer -d ./styles/prefixed",
    "prefix:css": "postcss --use autoprefixer --no-source-map false ./styles/main.min.css -o ./styles/main.prefix.css",

    "start": "npm-run-all --parallel devserver watch:sass",
    "build": "npm-run-all compile:sass prefix:css"
  },
  "browserslist": "last 10 versions",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.36",
    "postcss-cli": "^11.0.0",
    "sass": "^1.72.0"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
